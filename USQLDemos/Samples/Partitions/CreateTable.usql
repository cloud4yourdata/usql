USE DemoDb;
CREATE TABLE  IF NOT EXISTS dm.Orders
(
    OrderId int,
    OrderValue double,
    OrderDate DateTime,
    INDEX clx_OrderId
    CLUSTERED(OrderId)
    PARTITIONED BY (OrderDate)
    DISTRIBUTED BY
    HASH(OrderId)
    INTO 8
);

CREATE TABLE  IF NOT EXISTS dm.OrdersFailed
(
    OrderId int,
    OrderValue double,
    OrderDate DateTime,
    INDEX clx_OrderId
    CLUSTERED(OrderId)
    DISTRIBUTED BY
    HASH(OrderId)
    INTO 2
);